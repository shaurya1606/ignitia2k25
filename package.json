{
  "name": "ignitia2k25",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web",
    "build:web": "turbo run build --filter=web",
    "lint": "turbo run lint",
    "lint:web": "turbo run lint --filter=web",
    "format": "prettier --write \"apps/**/*.{ts,tsx,md}\" \"packages/**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "check-types:web": "turbo run check-types --filter=web",
    "ci": "turbo run lint check-types build --parallel"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}